<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.IDEFoundation.TextSubstitutionFileTemplateKind</string>
	<key>Description</key>
	<string>VIPER Module files.</string>
	<key>Summary</key>
	<string>VIPER Module files.</string>
	<key>SortOrder</key>
	<string>30</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.swift</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>The name of the module to create</string>
			<key>Identifier</key>
			<string>moduleName</string>
			<key>Name</key>
			<string>Module Name:</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>text</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_moduleName___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Description</key>
			<string>Base class for View Controller</string>
			<key>Identifier</key>
			<string>vcBaseClass</string>
			<key>Name</key>
			<string>View Controller Base Class:</string>
			<key>Required</key>
			<string>YES</string>
			<key>Type</key>
			<string>popup</string>
			<key>Values</key>
			<array>
				<string>VIPERViewController</string>
				<string>VIPERTableViewController</string>
				<string>VIPERCollectionViewController</string>
			</array>
			<key>Default</key>
			<string>VIPERViewController</string>
		</dict>
		<dict>
			<key>Description</key>
			<string>View Model type</string>
			<key>Identifier</key>
			<string>_multipleViewModels</string>
			<key>Name</key>
			<string>Multiple view models (otherwise is single)</string>
			<key>Type</key>
			<string>checkbox</string>
			<key>RequiredOptions</key>
			<dict>
				<key>vcBaseClass</key>
				<array>
					<string>VIPERViewController</string>
					<string>VIPERTableViewController</string>
					<string>VIPERCollectionViewController</string>
				</array>
			</dict>
		</dict>
	</array>
</dict>
</plist>
